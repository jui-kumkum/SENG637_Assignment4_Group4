        // Save the new address
        WebElement saveAddressButton = driver.findElement(By.xpath("//*[@id=\"topOfPage\"]/div[9]/div[1]/main/ul/li[1]/div/div/form/div/button')]"));
        saveAddressButton.click();

        // Edit the newly added address
        WebElement editAddressButton = driver.findElement(By.xpath("//*[@id=\"topOfPage\"]/div[9]/div[1]/main/ul/li[1]/div/div/form/div/a)]"));
